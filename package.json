{
  "name": "@mh4gf/shared-config",
  "version": "0.0.1",
  "license": "MIT",
  "author": "MH4GF <h.miyagi.cnw@gmail.com>",
  "private": true,
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@changesets/cli": "2.29.8",
    "@mh4gf/configs": "workspace:*",
    "@mh4gf/eslint-config": "workspace:*",
    "@types/node": "22.19.7",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.30.1",
    "knip": "5.81.0",
    "lefthook": "1.12.2",
    "scaffdog": "4.1.0",
    "syncpack": "13.0.4",
    "typescript": "5.9.3",
    "vitest": "3.2.4"
  },
  "scripts": {
    "build": "pnpm --filter '@mh4gf/*' build",
    "changeset": "changeset",
    "fmt": "pnpm run '/^fmt:.*/'",
    "fmt:biome": "biome check --write --unsafe .",
    "fmt:eslint": "eslint . --fix",
    "fmt:knip": "knip --fix",
    "fmt:syncpack-fix": "syncpack fix-mismatches",
    "fmt:syncpack-format": "syncpack format",
    "gen": "scaffdog generate",
    "lint": "pnpm run '/^lint:.*/'",
    "lint:biome": "biome check .",
    "lint:eslint": "eslint .",
    "lint:example": "pnpm --filter vite-project lint && pnpm --filter vite-project build",
    "lint:knip": "knip",
    "lint:syncpack": "syncpack lint",
    "prepare": "lefthook install",
    "release": "changeset publish",
    "test": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui"
  },
  "packageManager": "pnpm@10.28.0"
}
